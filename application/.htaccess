RewriteEngine On
RewriteBase /school/application/
RewriteCond $1 !^(index\.php|ajax|crawl|resources|robots\.txt)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^(.*)$ index.php/$1 [L]

RewriteRule ^ajax/([^/]+)/([^/]+)$ ajax.php/?path=$1&func=$2&%{QUERY_STRING} [L] 
RewriteRule ^ajax/([^/]+)$ ajax.php/?path=$1&%{QUERY_STRING} [L] 
